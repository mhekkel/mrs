    <path>../boost/include</path>
    <path>./libzeep</path>
  </syspaths>
  <userpaths>
    <path>./src</path>
    <path>./libzeep/src</path>
  </userpaths>
  <libpaths>
    <path>../boost/lib</path>
    <path>/usr/lib</path>
  </libpaths>
