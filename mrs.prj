  <targets>
    <target kind="Executable">
      <name>mrs-ws Debug</name>
      <linkTarget>mrs</linkTarget>
      <compiler>/usr/bin/c++</compiler>
      <defines>
        <define>DEBUG</define>
        <define>MRS_ETC_DIR=&quot;/etc/mrs&quot;</define>
        <define>MRS_USER=&quot;maarten&quot;</define>
      </defines>
      <cflags>
        <cflag>-g</cflag>
        <cflag>-O0</cflag>
        <cflag>-pthread</cflag>
        <cflag>-std=c++0x</cflag>
      </cflags>
      <ldflags>
        <ldflag>-pthread</ldflag>
      </ldflags>
      <warnings>
        <warning>all</warning>
        <warning>no-deprecated</warning>
        <warning>no-multichar</warning>
      </warnings>
    </target>
    <target kind="Executable">
      <name>mrs-ws Release</name>
      <linkTarget>mrs-ws</linkTarget>
      <compiler>/usr/bin/c++</compiler>
      <defines>
        <define>NDEBUG</define>
        <define>MRS_ETC_DIR=&quot;/etc/mrs&quot;</define>
        <define>MRS_USER=&quot;maarten&quot;</define>
      </defines>
      <cflags>
        <cflag>-g</cflag>
        <cflag>-pthread</cflag>
        <cflag>-std=c++0x</cflag>
        <cflag>-O3</cflag>
      </cflags>
      <ldflags>
        <ldflag>-pthread</ldflag>
      </ldflags>
      <warnings>
        <warning>all</warning>
        <warning>no-deprecated</warning>
        <warning>no-multichar</warning>
      </warnings>
    </target>
  </targets>
</project>
